import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) throws CheckInteger {
        Scanner in = new Scanner(System.in);
        String S = in.next();
        
        try{
            int i=Integer.parseInt(S);
            System.out.println(i);
        }catch(NumberFormatException e){
            System.out.println("Bad String");
        }
    }
}

class CheckInteger extends Exception
{
    public CheckInteger(String errorMessage)
    {
        super(errorMessage);
    }
}
